            <script type="text/javascript" src="/res/js/jquery.pagebar3.atag.js?t=1527124998905"></script>
            <div class="list_page" id="pagebar"></div>
            <script type="text/javascript">
                $(function () {
                    $.fn.jpagebar3({
                        renderTo: $("#pagebar"),
                        total: parseInt("20"),
                        pageSize: parseInt("10"),
                        currentPage: parseInt("1"),
                        pageNameClass: "pagebtn-name",
                        pageCountClass: "pagebtn-count",
                        pageList: [5, 10, 20, 30],
                        method: "default",
                        totalPage: "",
                        change: function (nextPage, pageSize, total) {
                            var type = $("#item_wrap1 .icon_yes").parent().attr("data-value");
                            var type2 = $("#item_wrap2 .icon_yes").parent().attr("data-value");
                            var px = $("#orderBy option:selected").val();
                            var searchKey = $.trim($("#searchInput").val());
                            if (searchKey != "" && searchKey != "输入关键词") {
                                location.href = "/page/s/cplist/index?search=" + encodeURIComponent(searchKey) + "&pageNo=" + nextPage + "&pageSize=" + pageSize + "&type="+type + "&type2="+type2 + "&px="+px;
                            } else {
                                location.href = "/page/s/cplist/index?pageNo=" + nextPage + "&pageSize=" + pageSize + "&type="+type + "&type2="+type2 + "&px="+px;
                            }
                        }
                    });
                })
            </script>
        
    </div>

</div>
</form>

<script>
    $(document).ready(function () {
        //回车焦点处理
        $("#searchInput").keyup(function (event) {
            var searchKey = $.trim($(this).val());
            if (event.keyCode == 13) {
                if ($(this).val() != "") {
                    location.href = "/page/s/cplist/index?search=" + encodeURIComponent(searchKey);
                } else if (false) {
                    location.href = "/page/s/cplist/index";
                }
            }
        });
        $("#searchBtn").click(function (event) {
            var searchKey = $.trim($("#searchInput").val());
            if ($("#searchInput").val() != "") {
                location.href = "/page/s/cplist/index?search=" + encodeURIComponent(searchKey);
            } else if (false) {
                location.href = "/page/s/cplist/index";
            }
        });


        $("#item_wrap1 .item_wrap_checked").each(function(){
            $(this).bind("click",function(){
                $("#item_wrap1 .checked_icon").each(function(){
                    $(this).removeClass("icon_yes");
                });
                $(this).find(".checked_icon").addClass("icon_yes");
                $("#fType").val( $(this).attr("data-value") );
                if( $("#searchInput").val()=='输入关键词' ){
                    $("#searchInput").val("");
                }
                $("#frm1").submit();
            })
        });

        $("#item_wrap2 .item_wrap_checked").each(function(){
            $(this).bind("click",function(){
                $("#item_wrap2 .checked_icon").each(function(){
                    $(this).removeClass("icon_yes");
                });
                $(this).find(".checked_icon").addClass("icon_yes");
                $("#fType2").val( $(this).attr("data-value") );
                if( $("#searchInput").val()=='输入关键词' ){
                    $("#searchInput").val("");
                }
                $("#frm1").submit();
            })
        });

        $(".title_search_btn").bind("click",function(){
            $("#fType").val("");
            $("#fType2").val("");
            if( $("#searchInput").val()=='输入关键词' ){
                $("#searchInput").val("");
            }
            $("#frm1").submit();
        });

        if( ""!="" ){
            $("#searchInput").val("");
        }

        $("#orderBy").change(function(){
            $("#px").val($(this).val());
            if( $("#searchInput").val()=='输入关键词' ){
                $("#searchInput").val("");
            }
            $("#frm1").submit();
        })
    })
</script>







