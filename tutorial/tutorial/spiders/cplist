    });
</script>
<script type="text/javascript">
    $(function () {
        var lHref = window.location.href;
        var lHost = window.location.host;
        var uPath = lHref.substring(lHref.indexOf(lHost)+lHost.length,lHref.length);
        if(uPath.indexOf('/page/gqzc/s') != -1 ){
            $("#navbar>ul>li.active").removeClass("active");
            $("#navbar>ul>li").eq(1).addClass('active');
        }else if(uPath.indexOf('/page/s/cplist')!= -1){
            $("#navbar>ul>li.active").removeClass("active");
            $("#navbar>ul>li").eq(2).addClass('active');
        }else if(uPath.indexOf('/page/qyzq/cplist')!= -1){
            $("#navbar>ul>li.active").removeClass("active");
            $("#navbar>ul>li").eq(3).addClass('active');
        }else if(uPath.indexOf('/page/xfzc/s/cplist')!= -1 ){
            $("#navbar>ul>li.active").removeClass("active");
            $("#navbar>ul>li").eq(4).addClass('active');
        }else if(uPath.indexOf('page/zcxmApply')!= -1 ){
            $("#navbar>ul>li.active").removeClass("active");
            $("#navbar>ul>li").eq(5).addClass('active');
        }else if(uPath.indexOf('undefined') != -1 ){
            $("#navbar>ul>li.active").removeClass("active");
            $("#navbar>ul>li").eq(6).addClass('active');
        }else if(uPath == '/' ){
            $("#navbar>ul>li.active").removeClass("active");
            $("#navbar>ul>li").eq(0).addClass('active');
        }else{
            $("#navbar>ul>li.active").removeClass("active");
        }
    })
</script>







